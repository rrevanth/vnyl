{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/app/*": [
        "./src/app/*"
      ],
      "@/config/*": [
        "./src/config/*"
      ],
      "@/infrastructure/*": [
        "./src/infrastructure/*"
      ],
      "@/data/*": [
        "./src/data/*"
      ],
      "@/domain/*": [
        "./src/domain/*"
      ],
      "@/presentation/*": [
        "./src/presentation/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "repos",
    "dist"
  ]
}
